{
  "name": "@wprdc-types/tag",
  "version": "0.0.11",
  "description": "Collection of TypeScript/JS libraries for use in/with WPRDC projects.",
  "keywords": [],
  "license": "AGPL-3.0-or-later",
  "author": "Steve Saylor <steven.saylor@pitt.edu>",
  "repository": {
    "type": "git",
    "url": "git@github.com:WPRDC/frontend-libraries.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/tag.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/tag.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/tag.esm.js",
      "limit": "10 KB"
    }
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^4.0.0",
    "@size-limit/preset-small-lib": "^7.0.5",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "autoprefixer": "^10.4.2",
    "husky": "^7.0.4",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "react": "18.1.0",
    "react-dom": "^18.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^7.0.5",
    "tailwindcss": "^3.0.18",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.7.3"
  }
}
