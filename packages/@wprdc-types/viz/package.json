{
  "name": "@wprdc-types/viz",
  "version": "0.0.25",
  "description": "Collection of TypeScript/JS libraries for use in/with WPRDC projects.",
  "keywords": [],
  "license": "AGPL-3.0-or-later",
  "author": "Steve Saylor <steven.saylor@pitt.edu>",
  "repository": {
    "type": "git",
    "url": "git@github.com:WPRDC/frontend-libraries.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/viz.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/viz.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/viz.esm.js",
      "limit": "10 KB"
    }
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^4.0.0",
    "@size-limit/preset-small-lib": "^8.0.1",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "husky": "^8.0.1",
    "postcss": "^8.4.16",
    "postcss-import": "^15.0.0",
    "postcss-loader": "^7.0.1",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^8.0.1",
    "tailwindcss": "^3.1.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@types/react": "^18.0.18",
    "@types/react-table": "^7.7.12",
    "@wprdc-types/geo": "^0.0.16",
    "@wprdc-types/map": "^0.0.17",
    "@wprdc-types/profiles": "^0.0.25",
    "@wprdc-types/shared": "^0.0.14",
    "react-map-gl": "^7.0.19"
  }
}
