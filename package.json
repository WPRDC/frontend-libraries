{
  "name": "wprdc-frontend-libraries",
  "private": true,
  "workspaces": [
    "packages/**/*",
    "apps/*"
  ],
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "bootstrap": "lerna clean -y && lerna bootstrap --hoist",
    "publish": "lerna publish",
    "link": "lerna link",
    "pretest": "eslint packages",
    "test": "jest",
    "build:lib": "lerna run prepare",
    "build:profiles-explorer": "yarn workspace profiles-explorer build",
    "watch:profiles-explorer": "yarn workspace profiles-explorer dev",
    "watch:components-am": "lerna run --scope '@wprdc-components/[a-m]*' --parallel watch",
    "watch:components-nz": "lerna run --scope '@wprdc-components/[n-z]*' --parallel watch",
    "watch:connections": "lerna run --scope '@wprdc-connections/*' --parallel watch",
    "watch:types-am": "lerna run --scope '@wprdc-types/[a-m]*' --parallel watch",
    "watch:types-nz": "lerna run --scope '@wprdc-types/[n-z]*' --parallel watch",
    "watch:viz": "lerna run --scope '@wprdc-viz/*' --parallel watch",
    "watch:widgets": "lerna run --scope '@wprdc-widgets/*' --parallel watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.4.9",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.12",
    "@storybook/manager-webpack5": "^6.4.12",
    "@storybook/react": "^6.4.9",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "postcss-import": "^14.0.2",
    "rollup": "^2.61.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.1"
  },
  "dependencies": {
    "@react-aria/breadcrumbs": "^3.1.5",
    "@react-aria/button": "^3.3.4",
    "@react-aria/checkbox": "^3.2.3",
    "@react-aria/combobox": "^3.2.0",
    "@react-aria/dialog": "^3.1.4",
    "@react-aria/focus": "^3.5.0",
    "@react-aria/i18n": "^3.3.4",
    "@react-aria/interactions": "^3.7.0",
    "@react-aria/label": "^3.2.1",
    "@react-aria/link": "^3.2.0",
    "@react-aria/listbox": "^3.4.0",
    "@react-aria/menu": "^3.3.0",
    "@react-aria/overlays": "^3.7.3",
    "@react-aria/radio": "^3.1.6",
    "@react-aria/searchfield": "^3.2.1",
    "@react-aria/select": "^3.6.0",
    "@react-aria/separator": "^3.1.3",
    "@react-aria/ssr": "^3.1.0",
    "@react-aria/tabs": "^3.1.0",
    "@react-aria/utils": "^3.10.0",
    "@react-stately/checkbox": "^3.0.3",
    "@react-stately/collections": "^3.3.4",
    "@react-stately/combobox": "^3.0.1",
    "@react-stately/data": "^3.4.3",
    "@react-stately/list": "^3.4.1",
    "@react-stately/overlays": "^3.1.3",
    "@react-stately/radio": "^3.3.2",
    "@react-stately/searchfield": "^3.1.3",
    "@react-stately/select": "^3.1.3",
    "@react-stately/tabs": "^3.0.1",
    "@react-stately/tree": "^3.2.0",
    "@react-types/breadcrumbs": "^3.2.1",
    "@react-types/checkbox": "^3.2.3",
    "@react-types/combobox": "^3.2.0",
    "@react-types/dialog": "^3.3.1",
    "@react-types/menu": "^3.4.1",
    "@react-types/overlays": "^3.5.1",
    "@react-types/radio": "^3.1.2",
    "@react-types/select": "^3.5.0",
    "@react-types/shared": "^3.10.1",
    "@react-types/tabs": "^3.0.1",
    "@types/estree": "^0.0.47",
    "@types/mapbox-gl": "^2.6.0",
    "@types/react-map-gl": "^6.1.1",
    "chroma-js": "^2.1.2",
    "classnames": "^2.3.1",
    "geojson": "^0.5.0",
    "next": "12.0.8",
    "next-transpile-modules": "^9.0.0",
    "react-measure": "^2.5.2",
